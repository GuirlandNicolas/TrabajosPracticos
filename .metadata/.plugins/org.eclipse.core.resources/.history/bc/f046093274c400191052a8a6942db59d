#include <stdio.h>
#include <stdlib.h>

int main(int) {
	int numero1;
	int numero2;
	int resultado ;

		scanf("Ingrese el primer numero : %d ",numero1);
		scanf("Ingrese el segundo numero : %d",numero2 );

		resultado=numero1+numero2;

		printf("El resultado es : %d",resultado);

		return 0 ;
}
